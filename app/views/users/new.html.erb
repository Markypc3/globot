<%= form_for @user do |f| %>
  <%= f.label :firstname %>
  <%= f.text_field :firstname %>
  <%= f.label :lastname %>
  <%= f.text_field :lastname %>
  <%= f.label :username%> The username must be unique. This may be the same as your email address if you like.
  <%= f.text_field :username %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password%>
  <%= f.password_field :password%>
  <%= f.label :password_confirmation%>
  <%= f.password_field :password_confirmation%>
  <%= f.submit "Sign up!"%>
<% end %>
