<%= form_for @timeslot do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :body %>
  <%= f.text_area :body %>
  <%= f.label :image_url %>
  <%= f.text_field :image_url %>
  <%= f.label :slot_start %>
  <%= f.datetime_select :slot_start %>
  <%= f.label :slot_end %>
  <%= f.datetime_select :slot_end %>
  <%= f.submit "Update Timeslot" %>
<% end %>
